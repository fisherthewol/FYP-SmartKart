<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <PreferenceCategory
            app:key="model_setting_category"
            app:title="@string/model_setting_category_text">
        <SeekBarPreference
            app:key="model_predict_time"
            app:title="@string/model_predict_time_text" 
            app:showSeekBarValue="true"
            app:defaultValue="@integer/predict_default"/>
        <SeekBarPreference
            app:key="max_speed_limit_mph"
            app:title="Maximum Speed Limit (MPH):"
            app:showSeekBarValue="true"
            app:defaultValue="@integer/speedlimit_default"/>
    </PreferenceCategory>
    
</PreferenceScreen>
